<div align='center'> 
  <%= link_to 'Back', users_path %>
  <h1>List of your liked Comments</h1><br>
  <br>
  <table border=1>
    <thead>
      <tr>  
        <th> Comments </th>
        <th> Operation </th>       
      </tr>
    </thead>

    <tbody>
      <% @likes.each do |like| %>
        <tr>
          <td> <%= like.comment.comment %> </td>
          <td> <%= link_to 'Dislike', dislike_path(like.id) %> </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>